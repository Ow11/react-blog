ge={ onTitleHandle } />\n            <label htmlFor=\"image\">Featured Image</label>\n            <ImageUploader />\n            <label htmlFor=\"content\">Content</label>\n            <textarea id=\"content\" placeholder=\"Supports markdown. Yay!\" value={ text } onChange={ onTextHandle } />\n        </div>\n    );\n}","/home/ow11/git/node.js-blogging-app-frontend-on-react/node.js-blogging-app-frontend/src/pages/admin-edit-article/index.ts",[],"/home/ow11/git/node.js-blogging-app-frontend-on-react/node.js-blogging-app-frontend/src/pages/admin-edit-article/admin-edit-article.tsx",[],"/home/ow11/git/node.js-blogging-app-frontend-on-react/node.js-blogging-app-frontend/src/pages/article-list/feed-mock.ts",[],{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","severity":1,"message":"123","line":1,"column":16,"nodeType":"124","messageId":"125","endLine":1,"endColumn":26},{"ruleId":"122","severity":1,"message":"126","line":1,"column":28,"nodeType":"124","messageId":"125","endLine":1,"endColumn":36},{"ruleId":"122","severity":1,"message":"127","line":1,"column":16,"nodeType":"124","messageId":"125","endLine":1,"endColumn":25},{"ruleId":"122","severity":1,"message":"128","line":9,"column":12,"nodeType":"124","messageId":"125","endLine":9,"endColumn":21},{"ruleId":"122","severity":1,"message":"129","line":9,"column":23,"nodeType":"124","messageId":"125","endLine":9,"endColumn":35},{"ruleId":"130","severity":1,"message":"131","line":17,"column":13,"nodeType":"132","endLine":17,"endColumn":78},"no-native-reassign",["133"],"no-negated-in-lhs",["134"],"@typescript-eslint/no-unused-vars","'useContext' is defined but never used.","Identifier","unusedVar","'useState' is defined but never used.","'useEffect' is defined but never used.","'selectAll' is assigned a value but never used.","'setSelectAll' is assigned a value but never used.","jsx-a11y/img-redundant-alt","Redundant alt attribute. Screen-readers already announce `img` tags as an image. 